{
  "name": "anq-phone-number-input",
  "version": "0.0.19",
  "description": "A powerful, customizable phone number input component for Quasar Framework and Vue 3 applications. Features include international phone number formatting, validation, and seamless integration with Quasar's design system.",
  "type": "module",
  "main": "./dist/anq-phone-number-input.umd.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "module": "./dist/anq-phone-number-input.js",
  "exports": {
    ".": {
      "import": "./dist/anq-phone-number-input.js",
      "require": "./dist/anq-phone-number-input.umd.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build-types.json --declaration --emitDeclarationOnly --outDir dist/types ",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "countries-list": "^3.1.1",
    "libphonenumber-js": "^1.11.16",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@quasar/extras": "^1.16.12",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "quasar": "^2.16.11",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.33.0",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "vue-tsc": "^2.0.29"
  },
  "keywords": [
    "anq-phone-number-input",
    "phone number",
    "quasar phone input",
    "vue phone input",
    "quasar framework",
    "vue 3",
    "phone number validation",
    "international phone",
    "phone input component",
    "quasar components",
    "vue components",
    "responsive phone input",
    "customizable phone input",
    "typescript phone input"
  ],
  "author": "aymane_nahji",
  "repository": {
    "type": "git",
    "url": "https://github.com/AymaneNahji/anq-phone-number-input"
  },
  "bugs": {
    "url": "https://github.com/AymaneNahji/anq-phone-number-input/issues"
  },
  "homepage": "https://github.com/AymaneNahji/anq-phone-number-input#readme",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "quasar": "^2.0.0",
    "vue": "^3.0.0"
  }
}
